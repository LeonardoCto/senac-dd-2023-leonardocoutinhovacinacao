CREATE TABLE `vacina`.`pessoa` (
  `idPESSOA` INT NOT NULL AUTO_INCREMENT,
  `Nome` VARCHAR(45) NOT NULL,
  `TipoPessoa` VARCHAR(45) NOT NULL,
  `CPF` VARCHAR(11) NOT NULL,
  `DataNascimento` DATE NOT NULL,
  `Sexo` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`idPESSOA`),
  UNIQUE INDEX `idPESSOA_UNIQUE` (`idPESSOA` ASC) VISIBLE,
  UNIQUE INDEX `CPF_UNIQUE` (`CPF` ASC) VISIBLE);
  
  CREATE TABLE `vacina`.`vacina` (
  `idVACINA` INT NOT NULL AUTO_INCREMENT,
  `EstagioPesquisa` VARCHAR(45) NOT NULL,
  `paisDeOrigem` VARCHAR(45) NOT NULL,
  `dataInicioPesquisa` DATE NOT NULL,
  `pesquisadorResposavel` VARCHAR(45) NOT NULL,
  `ID_PESSOA` INT NOT NULL,
  PRIMARY KEY (`idVACINA`),
  UNIQUE INDEX `idVACINA_UNIQUE` (`idVACINA` ASC) VISIBLE,
  UNIQUE INDEX `ID_PESSOA_UNIQUE` (`ID_PESSOA` ASC) VISIBLE,
  CONSTRAINT `ID_PESSOA`
    FOREIGN KEY (`ID_PESSOA`)
    REFERENCES `vacina`.`pessoa` (`idPESSOA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);